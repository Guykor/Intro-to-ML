\begin_layout Standard
יהי 
\begin_inset Formula $\mathcal{A}$
\end_inset

 אלגוריתם לומד, 
\begin_inset Formula $\mathcal{D}$
\end_inset

 התפלגות, ופונקציית ההפסד 
\begin_inset Formula $\ell^{0-1}$
\end_inset

.
 נוכיח כי הטענות הבאות שקולות:
\end_layout

\begin_layout Enumerate
לכל 
\begin_inset Formula $\varepsilon,\delta>0$
\end_inset

 קיימת פונקציה 
\begin_inset Formula $m\left(\varepsilon,\delta\right)\in\mathbb{R}$
\end_inset

 כך שלכל 
\begin_inset Formula $m\geq m\left(\varepsilon,\delta\right)$
\end_inset

 מתקיים 
\begin_inset Formula 
\[
\mathbb{P}_{S\sim\mathcal{D}^{m}}\left(L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\leq\varepsilon\right)\geq1-\delta
\]

\end_inset


\end_layout

\begin_layout Enumerate
מתקיים 
\begin_inset Formula $\text{lim}_{m\rightarrow\infty}\mathbb{E}_{S\sim\mathcal{D}^{m}}\left(L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\right)=0$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
הוכחה: 
\end_layout

\begin_layout Standard
ראשית, נסיק כי הטענות מוגדרות על בעיית סיווג, לומד 
\begin_inset Formula $\mathcal{A}:2^{\mathcal{X}}\rightarrow\mathcal{Y}^{\mathcal{X}}$
\end_inset

 )ללא הנחת קיום של מחלקת היפותזות(, ועם הנחת הרלייזביליות.
 כלומר, קיימת 
\begin_inset Formula $f:\mathcal{X}\rightarrow\mathcal{Y}\in\left\{ 0,1\right\} $
\end_inset

 עבורה נוכל לכתוב 
\begin_inset Formula $S=\left\{ \left(x_{i},f\left(x_{i}\right)\right)\right\} _{i=1}^{m}$
\end_inset

.
 כאשר כל 
\begin_inset Formula $X_{1},...,X_{m}\sim\mathcal{D}$
\end_inset

 לכל התפלגות 
\begin_inset Formula $\mathcal{D}$
\end_inset

 מעל 
\begin_inset Formula $\mathcal{X}$
\end_inset

, וגם 
\begin_inset Formula $L_{\mathcal{D}}\left(f\right)=0$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
בכיוון הראשון, 
\series default
יהיו 
\begin_inset Formula $\varepsilon,\delta>0$
\end_inset

 כך שקיימת 
\begin_inset Formula $m\left(\varepsilon,\delta\right)$
\end_inset

, עבורה לכל 
\begin_inset Formula $m\geq m\left(\varepsilon,\delta\right)$
\end_inset

 מתקיים 
\begin_inset Formula $\mathbb{P}_{S\sim\mathcal{D}^{m}}\left(L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\leq\varepsilon\right)\geq1-\delta$
\end_inset


\begin_inset Formula $(*)$
\end_inset

.
\end_layout

\begin_layout Standard
צ
\begin_inset Quotes erd
\end_inset

ל כי קיים 
\begin_inset Formula $\varepsilon_{0}>0$
\end_inset

 כך שקיים 
\begin_inset Formula $N\in\mathbb{N}$
\end_inset

 המקיים כי לכל 
\begin_inset Formula $m>N$
\end_inset

 מתקיים 
\begin_inset Formula $\left|\mathbb{E}_{S\sim\mathcal{D}^{m}}\left(L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\right)\right|<\varepsilon_{0}$
\end_inset

.
 ראשית, נבחין כי מאחר ו-
\begin_inset Formula $L_{\mathcal{D}}$
\end_inset

 מוגדרת עם 
\begin_inset Formula $\ell^{0-1}$
\end_inset

 אז 
\begin_inset Formula $L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)=\mathbb{P}_{X\sim\mathcal{D}}\left(h_{S}(x)\neq f(x)\right)$
\end_inset

, כלומר 
\begin_inset Formula $(\#)\;L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\in\left[0,1\right]$
\end_inset

.
\end_layout

\begin_layout Standard
מאחר ו-
\begin_inset Formula $S$
\end_inset

 הוא משתנה מקרי עם התפלגות 
\begin_inset Formula $\mathcal{D}^{m}$
\end_inset

, אז גם 
\begin_inset Formula $L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)$
\end_inset

 משתנה מקרי ולכן מהגדרת התוחלת מתקיים -
\begin_inset Formula 
\begin{align*}
\mathbb{E}_{S\sim\mathcal{D}^{m}}\left(L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\right) & =\underset{S\subseteq\mathcal{X}^{m}}{\int}L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\cdot f_{\mathcal{D}^{m}}\left(S\right)\\
 & \overset{\text{additivity}}{=}\underset{\underset{L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\leq\varepsilon}{S\subseteq\mathcal{X}^{m}}}{\int}L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\cdot f_{\mathcal{D}^{m}}\left(S\right)+\underset{\underset{L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)>\varepsilon}{S\subseteq\mathcal{X}^{m}}}{\int}L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\cdot f_{\mathcal{D}^{m}}\left(S\right)\\
 & \overset{(\#)L_{\mathcal{D}}\leq1}{\leq}\int_{_{L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\leq\varepsilon}}\varepsilon\cdot f_{\mathcal{D}^{m}}\left(S\right)+\int_{_{L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)>\varepsilon}}1\cdot f_{\mathcal{D}^{m}}\left(S\right)\\
 & \leq\varepsilon\cdot\int_{_{L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\leq\varepsilon}}\cdot f_{\mathcal{D}^{m}}\left(S\right)+\int_{L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)>\varepsilon}f_{\mathcal{D}^{m}}\left(S\right)\\
 & =\varepsilon\cdot\overset{\leq1}{\mathbb{P}_{S\sim\mathcal{D}^{m}}\left(L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\leq\varepsilon\right)}+\overset{(*)\:\leq\delta}{\mathbb{P}_{S\sim\mathcal{D}^{m}}\left(L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)>\varepsilon\right)}\\
 & \leq\varepsilon+\delta
\end{align*}

\end_inset

נגדיר 
\begin_inset Formula $\varepsilon_{0}=\varepsilon+\delta>0$
\end_inset

, 
\begin_inset Formula $N=m\left(\varepsilon,\delta\right)$
\end_inset

 וקיבלנו שלכל 
\begin_inset Formula $m\geq N$
\end_inset

 מתקיים 
\begin_inset Formula 
\[
\left|\mathbb{E}_{S\sim\mathcal{D}^{m}}\left(L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\right)\right|\leq\varepsilon_{0}
\]

\end_inset

כאשר השקילות לביטוי בערך מוחלט מאחר ו-
\begin_inset Formula $L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\in[0,1]$
\end_inset

, אז התוחלת תמיד אי-שלילית.
 
\begin_inset Formula $\blacksquare$
\end_inset


\end_layout

\begin_layout Standard

\series bold
בכיוון השני, 
\series default
יהיו 
\begin_inset Formula $\varepsilon,\delta>0$
\end_inset

.
 נגדיר 
\begin_inset Formula $\varepsilon_{0}=\varepsilon\delta>0$
\end_inset

, אז מההנחה, קיים 
\begin_inset Formula $N\in\mathbb{N}$
\end_inset

 כך שלכל 
\begin_inset Formula $m\geq N$
\end_inset

 מתקיים 
\begin_inset Formula $\left|\mathbb{E}_{S\sim\mathcal{D}^{m}}\left(L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\right)\right|\leq\varepsilon_{0}$
\end_inset

.
 כעת, מאחר ו- 
\begin_inset Formula $L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)$
\end_inset

 הינו פונקציה של המשתנה המקרי 
\begin_inset Formula $S$
\end_inset

, אזי שגיאת ההכללה הינה משתנה מקרי אי-שלילי.
 לפיכך, תנאי אי-שוויון מרקוב מתקיימים ולכן 
\begin_inset Formula 
\[
\mathbb{P}_{S\sim\mathcal{D}^{m}}\left(L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)>\varepsilon\right)\leq\frac{\mathbb{E}_{S\sim\mathcal{D}^{m}}\left(L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\right)}{\varepsilon}\leq\frac{\varepsilon_{0}}{\varepsilon}=\delta
\]

\end_inset


\end_layout

\begin_layout Standard
והמאורע המשלים הינו 
\begin_inset Formula $\mathbb{P}_{S\sim\mathcal{D}^{m}}\left(L_{\mathcal{D}}\left(\mathcal{A}\left(S\right)\right)\leq\varepsilon\right)\geq1-\delta$
\end_inset

 כנדרש.
 
\begin_inset Formula $\blacksquare$
\end_inset


