\begin_layout Standard

\series bold
שאלה 
\numeric on
4
\end_layout

\begin_layout Standard

\series bold
טענה: 
\series default
אם ל-
\begin_inset Formula $\mathcal{H}$
\end_inset

 מקיימת את תכונת התכנסות במ
\begin_inset Quotes erd
\end_inset

ש עם פונקציה 
\begin_inset Formula $m_{\mathcal{H}}^{UC}:\left(0,1\right)^{2}\rightarrow\mathbb{N}$
\end_inset

, אז 
\begin_inset Formula $\mathcal{H}$
\end_inset

 היא למידה 
\begin_inset Formula $\text{Agnostic-PAC}$
\end_inset

 עם סיבוכיות מדגם 
\begin_inset Formula $m_{\mathcal{H}}\left(\varepsilon,\delta\right)\leq m_{\mathcal{H}}^{UC}\left(\varepsilon/2,\delta\right)$
\end_inset

.
 
\end_layout

\begin_layout Standard

\series bold
הוכחה: 
\series default
יהיו 
\begin_inset Formula $0<\varepsilon,\delta<1$
\end_inset

, התפלגות 
\begin_inset Formula $\mathcal{D}$
\end_inset

 על 
\begin_inset Formula $\mathcal{X\times Y}$
\end_inset

 ופונקציית הפסד 
\begin_inset Formula $\ell$
\end_inset

 המגדירה את שגיאת ההכללה ואת השגיאה האמפירית.
 מהגדרה, מדגם 
\begin_inset Formula $S\subseteq\left(\mathcal{X\times Y}\right)^{m}$
\end_inset

 הינו 
\begin_inset Formula $\varepsilon$
\end_inset

-מייצג עבור 
\begin_inset Formula $\mathcal{D},\ell,\mathcal{H}$
\end_inset

 אם מתקיים 
\begin_inset Formula $\left|L_{S}\left(h\right)-L_{\mathcal{D}}\left(h\right)\right|<\varepsilon$
\end_inset

 לכל 
\begin_inset Formula $h\in\mathcal{H}$
\end_inset

 .
 באופן שקול, מתקיים 
\begin_inset Formula 
\begin{align*}
\left|L_{S}\left(h\right)-L_{\mathcal{D}}\left(h\right)\right|<\varepsilon & \iff L_{\mathcal{D}}\left(h\right)-\frac{\varepsilon}{2}<L_{S}\left(h\right)<L_{\mathcal{D}}\left(h\right)+\frac{\varepsilon}{2}\\
 & \iff L_{\mathcal{D}}\left(h\right)<L_{S}\left(h\right)+\frac{\varepsilon}{2}<L_{\mathcal{D}}\left(h\right)+\varepsilon
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
נסמן ב-
\begin_inset Formula $h_{S}$
\end_inset

 את הפלט של אלגוריתם הלמידה 
\begin_inset Formula $ERM_{\mathcal{H}}\left(S\right)$
\end_inset

, כלומר 
\begin_inset Formula $h_{S}=\underset{h\in\mathcal{H}}{\text{argmin}}\left(L_{S}\left(h\right)\right)$
\end_inset

, וב-
\begin_inset Formula $h^{*}=\underset{h\in\mathcal{H}}{\text{argmin}}\left(L_{\mathcal{D}}\left(h\right)\right)$
\end_inset

 את המסווג שמשיג את שגיאת ההכללה המינימלית.
 נבחין כי מתקיים עבור מדגם 
\begin_inset Formula $S$
\end_inset

 
\begin_inset Formula $\frac{\varepsilon}{2}$
\end_inset

-מייצג מתקיים 
\begin_inset Formula $L_{\mathcal{D}}\left(h_{S}\right)<L_{S}\left(h_{S}\right)+\frac{\varepsilon}{2}$
\end_inset

 וגם 
\begin_inset Formula $L_{S}\left(h^{*}\right)+\frac{\varepsilon}{2}<L_{\mathcal{D}}\left(h^{*}\right)+\varepsilon$
\end_inset

.
 כמו כן, מאופן פעולת 
\begin_inset Formula $ERM_{\mathcal{H}}$
\end_inset

, מתקיים כי 
\begin_inset Formula $(*)\:L_{S}\left(h_{S}\right)\leq L_{S}\left(h^{*}\right)$
\end_inset

 ולכן בסה
\begin_inset Quotes erd
\end_inset

כ מתקיים 
\begin_inset Formula 
\[
L_{\mathcal{D}}\left(h_{S}\right)<L_{S}\left(h_{S}\right)+\frac{\varepsilon}{2}\overset{(*)}{\leq}L_{S}\left(h^{*}\right)+\frac{\varepsilon}{2}<L_{\mathcal{D}}\left(h^{*}\right)+\varepsilon
\]

\end_inset

 כלומר, מתקיים 
\begin_inset Formula $L_{\mathcal{D}}\left(h_{S}\right)\leq\underset{h\in\mathcal{H}}{\text{min}}L_{\mathcal{D}}\left(h\right)+\varepsilon$
\end_inset

 )נוכל לעדן לא
\begin_inset Quotes erd
\end_inset

ש חלש(.
 הראינו כי 
\begin_inset Formula 
\[
S_{0}\in\left\{ S\subseteq\mathcal{X\times Y}:\text{S is }\frac{\varepsilon}{2}\text{-representative }\right\} \Rightarrow S_{0}\in\left\{ S\subseteq\mathcal{X\times Y}:L_{\mathcal{D}}\left(h_{S}\right)\leq\underset{h\in\mathcal{H}}{\text{min}}L_{\mathcal{D}}\left(h\right)+\varepsilon\right\} 
\]

\end_inset

 ולכן 
\begin_inset Formula $\left\{ L_{\mathcal{D}}\left(h_{S}\right)\leq\underset{h\in\mathcal{H}}{\text{min}}L_{\mathcal{D}}\left(h\right)+\varepsilon\right\} \supseteq\left\{ \text{S is }\frac{\varepsilon}{2}\text{-representative }\right\} $
\end_inset

 
\begin_inset Formula $(**)$
\end_inset

.
\end_layout

\begin_layout Standard
כעת, מאחר ו-
\begin_inset Formula $\mathcal{H}$
\end_inset

 מקיימת את תכונת התכנסות במידה שווה, אז קיימת פונקציה 
\begin_inset Formula $m_{\mathcal{H}}^{UC}$
\end_inset

 כך שלכל 
\begin_inset Formula $m\in\mathbb{N}$
\end_inset

 המקיים 
\begin_inset Formula $m\geq m_{\mathcal{H}}^{UC}\left(\frac{\varepsilon}{2},\delta\right)$
\end_inset

 מתקיים 
\begin_inset Formula 
\[
\mathbb{P}_{S\sim\mathcal{D}^{m}}\left(L_{\mathcal{D}}\left(h_{S}\right)\leq\underset{h\in\mathcal{H}}{\text{min}}L_{\mathcal{D}}\left(h\right)+\varepsilon\right)\overset{(**)}{\geq}\mathbb{P}_{S\sim\mathcal{D}^{m}}\left(S\text{ is }\frac{\varepsilon}{2}\text{-representative}\right)\geq1-\delta
\]

\end_inset

כלומר, 
\begin_inset Formula $\mathcal{H}$
\end_inset

 למידה 
\begin_inset Formula $\text{Agnostic-PAC}$
\end_inset

, עם סיבוכיות מדגם 
\begin_inset Formula $m_{\mathcal{H}}\left(\varepsilon,\delta\right)\leq m_{\mathcal{H}}^{UC}\left(\frac{\varepsilon}{2},\delta\right)$
\end_inset

.
 כלומר, 
\begin_inset Formula $\mathcal{H}$
\end_inset

 המקיימת את תכונת התכנסות במ
\begin_inset Quotes erd
\end_inset

ש, מקיימת את חוק המספרים הגדולים )
\begin_inset Formula $L_{S}$
\end_inset

 מתכנס בהסתברות ל-
\begin_inset Formula $L_{\mathcal{D}}$
\end_inset

( לכל 
\begin_inset Formula $\mathcal{D},\varepsilon,\delta$
\end_inset

 ולכן בפרט קיים חסם לשגיאת ההכללה.
 
\begin_inset Formula $\blacksquare$
\end_inset


